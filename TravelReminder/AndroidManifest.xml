<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ganterd.travelreminder"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />
    
    <meta-data android:name="com.google.android.gms.version"
           android:value="@integer/google_play_services_version" />
    
    <uses-permission android:name="android.permission.WRITE_INTERNAL_STROAGE"/>
    <uses-permission android:name="android.permission.READ_INTERNAL_STROAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.ganterd.travelreminder.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.ganterd.travelreminder.CreateTravelReminderActivity"
            android:label="@string/title_activity_create_travel_reminder"
            android:parentActivityName="com.ganterd.travelreminder.MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.travelreminder.MainActivity" />
        </activity>
    </application>

</manifest>
